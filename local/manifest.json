{
    "_version": "1.7.0",
    "sap.app": {
        "id": "edu.mit.voip-portal",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "sourceTemplate": {
            "id": "ui5template.basicSAPUI5ApplicationProject",
            "version": "1.40.12"
        }
    },

    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_hcb",
            "sap_belize"
        ]
    },

    "sap.ui5": {
        "rootView": {
            "viewName": "edu.mit.voip-portal.features.App",
            "type": "XML",
            "async": true
        },
        "dependencies": {
            "minUI5Version": "1.30.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.ui.layout": {},
                "sap.ushell": {},
                "sap.collaboration": {},
                "sap.ui.comp": {},
                "sap.uxap": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "edu.mit.voip-portal.i18n.i18n"
                }
            }
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "async": true,
                "viewType": "XML",
                "viewPath": "edu.mit.voip-portal.features",
                "controlId": "app",
                "controlAggregation": "pages"
            },
            "routes": [{
                    "pattern": "",
                    "name": "dashboardaa",
                    "target": "dashboardaa"
                }
            ],
            "targets": {
                "home": {
                    "viewName": "home.Home",
                    "transition": "show"
                },
                "dashboardaa": {
					"viewName": "dashboardaa.Dashboardaa",
					"transition": "show"
				}
                
            }
        },
        "resources": {
            "css": [{
                "uri": "css/style.css"
            },
            {
                "uri": "css/less.css"
            }]
        }
    }
}