<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml"
	controllerName="edu.mit.voip-portal.features.dashboardaa.Dashboardaa">
	<App>
		<pages>
			<Page title="American Airlines Flights">
				<content>
					<Table noDataText="No Flights Found" id="table0" items="{flights>/flightlist}">
						<items>
							<ColumnListItem type="Active" id="item0">
								<cells>
									<Text text="{flights>ID}" id="text0"/>
									<Text text="{flights>origin}" id="text1"/>
									<Text text="{flights>destination}" id="text2"/>
								</cells>
							</ColumnListItem>
						</items>
						<columns>
							<Column id="column0">
								<header>
									<Label text="Flight ID" id="label0"/>
								</header>
							</Column>
							<Column id="column1">
								<header>
									<Label text="Origin" id="label1"/>
								</header>
							</Column>
							<Column id="column2">
								<header>
									<Label text="Destination" id="label2"/>
								</header>
							</Column>
						</columns>
					</Table>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>